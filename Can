local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ–∫–Ω–∞
local Window = Rayfield:CreateWindow({
    Name = "–ü—Ä–æ—Å–ª—É—à–∫–∞",
    LoadingTitle = "–ü—Ä–æ—Å–ª—É—à–∫–∞",
    LoadingSubtitle = "by Can1488",
    ConfigurationSaving = {
        Enabled = false,
    },
    Discord = {
        Enabled = false,
    },
    KeySystem = false,
})

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤–∫–ª–∞–¥–∫–∏
local MainTab = Window:CreateTab("–ì–ª–∞–≤–Ω–∞—è", nil)

-- –û—Å–Ω–æ–≤–Ω—ã–µ —Å–µ—Ä–≤–∏—Å—ã
local Players = game:GetService("Players")
local SoundService = game:GetService("SoundService")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è
local SelectedPlayers = {}
local isSpying = false
local spyConnection = nil
local currentSpyIndex = 1

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local VoicechatSpySettings = {
    TrackVoice3d = true,
    FollowDistance = 10
}

-- –°–æ–∑–¥–∞–Ω–∏–µ —Ä–∞–∑–¥–µ–ª–∞ —Å –Ω–∞—Å—Ç—Ä–æ–π–∫–∞–º–∏
local SettingsSection = MainTab:CreateSection("–ù–∞—Å—Ç—Ä–æ–π–∫–∏")

local TrackVoice3dToggle = MainTab:CreateToggle({
    Name = "Track Voice 3D",
    CurrentValue = VoicechatSpySettings.TrackVoice3d,
    Callback = function(Value)
        VoicechatSpySettings.TrackVoice3d = Value
    end,
})

local FollowDistanceSlider = MainTab:CreateSlider({
    Name = "Follow Distance",
    Range = {1, 50},
    Increment = 1,
    Suffix = "studs",
    CurrentValue = VoicechatSpySettings.FollowDistance,
    Callback = function(Value)
        VoicechatSpySettings.FollowDistance = Value
    end,
})

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function GetPlayerList()
    local playerNames = {}
    
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(playerNames, player.Name)
        end
    end
    
    if #playerNames == 0 then
        table.insert(playerNames, "–ù–µ—Ç –∏–≥—Ä–æ–∫–æ–≤")
    end
    
    return playerNames
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–∞ –ø–æ –∏–º–µ–Ω–∏
local function FindPlayerByName(name)
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name == name then
            return player
        end
    end
    return nil
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –¥—Ä–æ–ø–¥–∞—É–Ω–∞ –¥–ª—è –≤—ã–±–æ—Ä–∞ –∏–≥—Ä–æ–∫–æ–≤
local PlayersSection = MainTab:CreateSection("–í—ã–±–æ—Ä —Ü–µ–ª–µ–π (Ctrl –¥–ª—è –º–Ω–æ–∂–µ—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ –≤—ã–±–æ—Ä–∞)")

local PlayerDropdown = MainTab:CreateDropdown({
    Name = "–í—ã–±–µ—Ä–∏—Ç–µ —Ü–µ–ª–∏",
    Options = GetPlayerList(),
    CurrentOption = {},
    MultipleOptions = true,
    Callback = function(Options)
        -- –û—á–∏—â–∞–µ–º —Ç–µ–∫—É—â–∏–π —Å–ø–∏—Å–æ–∫ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
        SelectedPlayers = {}
        
        -- –î–æ–±–∞–≤–ª—è–µ–º —Ç–æ–ª—å–∫–æ —Ä–µ–∞–ª—å–Ω—ã—Ö –∏–≥—Ä–æ–∫–æ–≤ (–Ω–µ "–ù–µ—Ç –∏–≥—Ä–æ–∫–æ–≤")
        for _, playerName in pairs(Options) do
            if playerName ~= "–ù–µ—Ç –∏–≥—Ä–æ–∫–æ–≤" then
                local player = FindPlayerByName(playerName)
                if player then
                    table.insert(SelectedPlayers, player)
                end
            end
        end
        
        UpdateSelectedCount()
    end,
})

-- –°—á–µ—Ç—á–∏–∫ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
local SelectedCountLabel = MainTab:CreateLabel("–í—ã–±—Ä–∞–Ω–æ: 0 —Ü–µ–ª–µ–π")

-- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—á–µ—Ç—á–∏–∫–∞ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö
local function UpdateSelectedCount()
    local count = #SelectedPlayers
    SelectedCountLabel:Set("–í—ã–±—Ä–∞–Ω–æ: " .. count .. " —Ü–µ–ª–µ–π")
end

-- –§—É–Ω–∫—Ü–∏—è –∑–∞–ø—É—Å–∫–∞ –ø—Ä–æ—Å–ª—É—à–∫–∏
local function StartSpying()
    if isSpying then
        Rayfield:Notify({
            Title = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è",
            Content = "–ü—Ä–æ—Å–ª—É—à–∫–∞ —É–∂–µ –∑–∞–ø—É—â–µ–Ω–∞",
            Duration = 3,
        })
        return
    end
    
    if #SelectedPlayers == 0 then
        Rayfield:Notify({
            Title = "–û—à–∏–±–∫–∞",
            Content = "–ù–µ –≤—ã–±—Ä–∞–Ω–æ –Ω–∏ –æ–¥–Ω–æ–π —Ü–µ–ª–∏",
            Duration = 3,
        })
        return
    end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –≤—Å–µ –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –∏–≥—Ä–æ–∫–∏ —Å—É—â–µ—Å—Ç–≤—É—é—Ç
    local validPlayers = {}
    for _, player in ipairs(SelectedPlayers) do
        if player and player.Parent then

table.insert(validPlayers, player)
        end
    end
    
    if #validPlayers == 0 then
        Rayfield:Notify({
            Title = "–û—à–∏–±–∫–∞",
            Content = "–í—ã–±—Ä–∞–Ω–Ω—ã–µ –∏–≥—Ä–æ–∫–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã",
            Duration = 3,
        })
        return
    end
    
    SelectedPlayers = validPlayers
    isSpying = true
    currentSpyIndex = 1
    
    -- –ó–∞–ø—É—Å–∫–∞–µ–º –ø—Ä–æ—Å–ª—É—à–∫—É —á–µ—Ä–µ–∑ Heartbeat
    if spyConnection then
        spyConnection:Disconnect()
        spyConnection = nil
    end
    
    spyConnection = RunService.Heartbeat:Connect(function()
        if not isSpying or #SelectedPlayers == 0 then
            return
        end
        
        -- –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞ –¥–ª—è –ø—Ä–æ—Å–ª—É—à–∫–∏
        local player = SelectedPlayers[currentSpyIndex]
        
        if player and player.Parent then
            local character = player.Character
            if character then
                local hrp = character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Å–ª—É—à–∞—Ç–µ–ª—è –Ω–∞ –ø–æ–∑–∏—Ü–∏—é –∏–≥—Ä–æ–∫–∞
                    if VoicechatSpySettings.TrackVoice3d then
                        SoundService:SetListener(
                            Enum.ListenerType.CFrame,
                            hrp.CFrame + (hrp.CFrame.lookVector * VoicechatSpySettings.FollowDistance)
                        )
                    else
                        SoundService:SetListener(Enum.ListenerType.CFrame, hrp.CFrame)
                    end
                else
                    SoundService:SetListener(Enum.ListenerType.Camera)
                end
            else
                SoundService:SetListener(Enum.ListenerType.Camera)
            end
        else
            SoundService:SetListener(Enum.ListenerType.Camera)
            -- –£–¥–∞–ª—è–µ–º –Ω–µ—Å—É—â–µ—Å—Ç–≤—É—é—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞
            table.remove(SelectedPlayers, currentSpyIndex)
            if #SelectedPlayers == 0 then
                isSpying = false
                return
            end
            if currentSpyIndex > #SelectedPlayers then
                currentSpyIndex = 1
            end
        end
        
        -- –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –∏–≥—Ä–æ–∫—É (—Ü–∏–∫–ª–∏—á–µ—Å–∫–æ–µ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ)
        currentSpyIndex = currentSpyIndex + 1
        if currentSpyIndex > #SelectedPlayers then
            currentSpyIndex = 1
        end
        
        UpdateSpyStatus()
    end)
    
    Rayfield:Notify({
        Title = "–£—Å–ø–µ—à–Ω–æ",
        Content = "–ü—Ä–æ—Å–ª—É—à–∫–∞ –∑–∞–ø—É—â–µ–Ω–∞ –¥–ª—è " .. #SelectedPlayers .. " —Ü–µ–ª–µ–π",
        Duration = 3,
    })
    
    UpdateSpyStatus()
end

-- –§—É–Ω–∫—Ü–∏—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∏ –ø—Ä–æ—Å–ª—É—à–∫–∏
local function StopSpying()
    if not isSpying then
        Rayfield:Notify({
            Title = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è",
            Content = "–ü—Ä–æ—Å–ª—É—à–∫–∞ —É–∂–µ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞",
            Duration = 3,
        })
        return
    end
    
    isSpying = false
    
    if spyConnection then
        spyConnection:Disconnect()
        spyConnection = nil
    end
    
    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Å–ª—É—à–∞—Ç–µ–ª—è –∫ –∫–∞–º–µ—Ä–µ
    SoundService:SetListener(Enum.ListenerType.Camera)
    
    Rayfield:Notify({
        Title = "–£—Å–ø–µ—à–Ω–æ",
        Content = "–ü—Ä–æ—Å–ª—É—à–∫–∞ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞",
        Duration = 3,
    })
    
    UpdateSpyStatus()
end

-- –ö–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø—Ä–æ—Å–ª—É—à–∫–æ–π
local ControlSection = MainTab:CreateSection("–£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø—Ä–æ—Å–ª—É—à–∫–æ–π")

local StartSpyButton = MainTab:CreateButton({
    Name = "‚ñ∂ –ó–∞–ø—É—Å—Ç–∏—Ç—å –ø—Ä–æ—Å–ª—É—à–∫—É",
    Callback = function()
        StartSpying()
    end,
})

local StopSpyButton = MainTab:CreateButton({
    Name = "‚èπ –û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –ø—Ä–æ—Å–ª—É—à–∫—É",
    Callback = function()
        StopSpying()
    end,
})

-- –°–µ–∫—Ü–∏—è —Å –≥—Ä—É–ø–ø–æ–≤—ã–º–∏ –¥–µ–π—Å—Ç–≤–∏—è–º–∏
local GroupSection = MainTab:CreateSection("–ì—Ä—É–ø–ø–æ–≤—ã–µ –¥–µ–π—Å—Ç–≤–∏—è")

local SelectAllButton = MainTab:CreateButton({
    Name = "–í—ã–±—Ä–∞—Ç—å –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤",
    Callback = function()

SelectedPlayers = {}
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                table.insert(SelectedPlayers, player)
            end
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –¥—Ä–æ–ø–¥–∞—É–Ω
        local selectedNames = {}
        for _, player in ipairs(SelectedPlayers) do
            table.insert(selectedNames, player.Name)
        end
        
        PlayerDropdown:Set(selectedNames)
        UpdateSelectedCount()
        
        Rayfield:Notify({
            Title = "–£—Å–ø–µ—à–Ω–æ",
            Content = "–í—ã–±—Ä–∞–Ω—ã –≤—Å–µ –∏–≥—Ä–æ–∫–∏ (" .. #SelectedPlayers .. " —Ü–µ–ª–µ–π)",
            Duration = 3,
        })
    end,
})

local ClearSelectionButton = MainTab:CreateButton({
    Name = "–û—á–∏—Å—Ç–∏—Ç—å –≤—ã–±–æ—Ä",
    Callback = function()
        SelectedPlayers = {}
        PlayerDropdown:Set({})
        UpdateSelectedCount()
        
        Rayfield:Notify({
            Title = "–£—Å–ø–µ—à–Ω–æ",
            Content = "–í—ã–±–æ—Ä –æ—á–∏—â–µ–Ω",
            Duration = 3,
        })
    end,
})

local SpyAllButton = MainTab:CreateButton({
    Name = "üéØ –ü—Ä–æ—Å–ª—É—à–∏–≤–∞—Ç—å –≤—Å–µ—Ö",
    Callback = function()
        -- –í—ã–±–∏—Ä–∞–µ–º –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
        SelectedPlayers = {}
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                table.insert(SelectedPlayers, player)
            end
        end
        
        if #SelectedPlayers == 0 then
            Rayfield:Notify({
                Title = "–û—à–∏–±–∫–∞",
                Content = "–ù–µ—Ç –∏–≥—Ä–æ–∫–æ–≤ –¥–ª—è –ø—Ä–æ—Å–ª—É—à–∫–∏",
                Duration = 3,
            })
            return
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –¥—Ä–æ–ø–¥–∞—É–Ω
        local selectedNames = {}
        for _, player in ipairs(SelectedPlayers) do
            table.insert(selectedNames, player.Name)
        end
        
        PlayerDropdown:Set(selectedNames)
        UpdateSelectedCount()
        
        -- –ó–∞–ø—É—Å–∫–∞–µ–º –ø—Ä–æ—Å–ª—É—à–∫—É
        StartSpying()
    end,
})

local UnspyAllButton = MainTab:CreateButton({
    Name = "üö´ –û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –≤—Å—é –ø—Ä–æ—Å–ª—É—à–∫—É",
    Callback = function()
        StopSpying()
        SelectedPlayers = {}
        PlayerDropdown:Set({})
        UpdateSelectedCount()
    end,
})

-- –ö–Ω–æ–ø–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å–ø–∏—Å–∫–∞
local RefreshButton = MainTab:CreateButton({
    Name = "üîÑ –û–±–Ω–æ–≤–∏—Ç—å —Å–ø–∏—Å–æ–∫",
    Callback = function()
        local playerList = GetPlayerList()
        PlayerDropdown:Refresh(playerList, true)
        
        Rayfield:Notify({
            Title = "–û–±–Ω–æ–≤–ª–µ–Ω–æ",
            Content = "–°–ø–∏—Å–æ–∫ –∏–≥—Ä–æ–∫–æ–≤ –æ–±–Ω–æ–≤–ª–µ–Ω",
            Duration = 2,
        })
    end,
})

-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
local InfoSection = MainTab:CreateSection("–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è")

local SpyStatusLabel = MainTab:CreateLabel("–°—Ç–∞—Ç—É—Å: –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ")
local ActiveTargetsLabel = MainTab:CreateLabel("–ü—Ä–æ—Å–ª—É—à–∏–≤–∞–µ—Ç—Å—è: 0 —Ü–µ–ª–µ–π")

-- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞
local function UpdateSpyStatus()
    if isSpying then
        SpyStatusLabel:Set("–°—Ç–∞—Ç—É—Å: ‚ñ∂ –ó–∞–ø—É—â–µ–Ω–æ")
        ActiveTargetsLabel:Set("–ü—Ä–æ—Å–ª—É—à–∏–≤–∞–µ—Ç—Å—è: " .. #SelectedPlayers .. " —Ü–µ–ª–µ–π")
    else
        SpyStatusLabel:Set("–°—Ç–∞—Ç—É—Å: ‚èπ –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ")
        ActiveTargetsLabel:Set("–ü—Ä–æ—Å–ª—É—à–∏–≤–∞–µ—Ç—Å—è: 0 —Ü–µ–ª–µ–π")
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π –∏–≥—Ä–æ–∫–æ–≤
Players.PlayerAdded:Connect(function(player)
    wait(1)
    local playerList = GetPlayerList()
    PlayerDropdown:Refresh(playerList, true)
end)

Players.PlayerRemoving:Connect(function(player)
    -- –£–¥–∞–ª—è–µ–º –∏–≥—Ä–æ–∫–∞ –∏–∑ —Å–ø–∏—Å–∫–∞ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö
    for i, selectedPlayer in ipairs(SelectedPlayers) do
        if selectedPlayer == player then
            table.remove(SelectedPlayers, i)
            break
        end
    end
    
    -- –ï—Å–ª–∏ –ø—Ä–æ—Å–ª—É—à–∫–∞ –∞–∫—Ç–∏–≤–Ω–∞ –∏ —Å–ø–∏—Å–æ–∫ –ø—É—Å—Ç, –æ—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º
    if isSpying and #SelectedPlayers == 0 then
        StopSpying()
    end
    
    UpdateSelectedCount()
    UpdateSpyStatus()
end)

-- –ö–Ω–æ–ø–∫–∞ –≤—ã—Ö–æ–¥–∞
local ExitSection = MainTab:CreateSection("–í—ã—Ö–æ–¥")

local ExitButton = MainTab:CreateButton({
    Name = "–í—ã—Ö–æ–¥",
    Callback = function()
        StopSpying()
        Rayfield:Destroy()
    end,
})

-- –§—É–Ω–∫—Ü–∏—è —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—è –ø—Ä–æ—Å–ª—É—à–∫–∏
local TestSection = MainTab:CreateSection("–¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ")

local TestButton = MainTab:CreateButton({
    Name = "–¢–µ—Å—Ç –ø—Ä–æ—Å–ª—É—à–∫–∏",
    Callback = function()
        if #SelectedPlayers == 0 then
            Rayfield:Notify({
                Title = "–¢–µ—Å—Ç",
                Content = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–≥—Ä–æ–∫–∞ –¥–ª—è —Ç–µ—Å—Ç–∞",
                Duration = 3,
            })
            return
        end
        
        local testPlayer = SelectedPlayers[1]
        if testPlayer and testPlayer.Parent then
            Rayfield:Notify({
                Title = "–¢–µ—Å—Ç",
                Content = "–¢–µ—Å—Ç –ø—Ä–æ—Å–ª—É—à–∫–∏ –¥–ª—è " .. testPlayer.Name,
                Duration = 3,
            })
            
            -- –í—Ä–µ–º–µ–Ω–Ω–æ –≤–∫–ª—é—á–∞–µ–º –ø—Ä–æ—Å–ª—É—à–∫—É –Ω–∞ 3 —Å–µ–∫—É–Ω–¥—ã
            local originalState = isSpying
            if not originalState then
                StartSpying()
                wait(3)
                StopSpying()
            else
                wait(3)
            end
        else
            Rayfield:Notify({
                Title = "–û—à–∏–±–∫–∞ —Ç–µ—Å—Ç–∞",
                Content = "–ò–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω",
                Duration = 3,
            })
        end
    end,
})

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
wait(2)
UpdateSelectedCount()
UpdateSpyStatus()

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è –ª–∏ –≥–æ–ª–æ—Å–æ–≤–æ–π —á–∞—Ç –≤ –∏–≥—Ä–µ
local function CheckVoiceChatSupport()
    local success, result = pcall(function()
        return SoundService:GetPropertyChangedSignal("ListenerType")
    end)
    
    if success then
        print("–ì–æ–ª–æ—Å–æ–≤–æ–π —á–∞—Ç –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è")
        return true
    else
        print("–ì–æ–ª–æ—Å–æ–≤–æ–π —á–∞—Ç –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è")
        Rayfield:Notify({
            Title = "–í–Ω–∏–º–∞–Ω–∏–µ",
            Content = "–ì–æ–ª–æ—Å–æ–≤–æ–π —á–∞—Ç –º–æ–∂–µ—Ç –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞—Ç—å—Å—è –≤ —ç—Ç–æ–π –∏–≥—Ä–µ",
            Duration = 5,
        })
        return false
    end
end

CheckVoiceChatSupport()

-- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –æ –∑–∞–ø—É—Å–∫–µ
Rayfield:Notify({
    Title = "–ü—Ä–æ—Å–ª—É—à–∫–∞ –≥–æ–ª–æ—Å–æ–≤–æ–≥–æ —á–∞—Ç–∞",
    Content = "–í—ã–±–µ—Ä–∏—Ç–µ —Ü–µ–ª–∏ –∏ –∑–∞–ø—É—Å—Ç–∏—Ç–µ –ø—Ä–æ—Å–ª—É—à–∫—É",
    Duration = 5,
})

-- –ò–Ω—Å—Ç—Ä—É–∫—Ü–∏—è –≤ –∫–æ–Ω—Å–æ–ª—å
print("========================================")
print("–ü–†–û–°–õ–£–®–ö–ê –ì–û–õ–û–°–û–í–û–ì–û –ß–ê–¢–ê")
